{
  "name": "pagopa-wallet-fe",
  "version": "0.1.0",
  "repository": "https://github.com/pagopa/pagopa-wallet-fe.git",
  "author": "PagoPA SpA",
  "license": "MIT",
  "scripts": {
    "generate": "npm-run-all generate:*",
    "generate:webview-payment-wallet-api": "rimraf generated/definitions/webview-payment-wallet && shx mkdir -p generated/definitions/webview-payment-wallet && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/bbb481ca463b531dc21844ec18f49c4a7a41de4b/src/domains/wallet-app/api/webview-payment-wallet/v1/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/webview-payment-wallet --request-types --response-decoders --client",
    "generate:payment-manager-api-v1": "rimraf generated/definitions/payment-manager-v1 && shx mkdir -p generated/definitions/payment-manager-v1 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-app/4e08abd3f395f6c4de91613704523a7415ba7e47/assets/paymentManager/spec.json --no-strict --out-dir ./generated/definitions/payment-manager-v1 --request-types --response-decoders --client",
    "type-check": "tsc",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "clean": "rimraf .cache && rimraf dist && shx rm -rf src/generated/*  && shx rm -rf .parcel-cache && shx rm -rf node_modules",
    "prebuild": "npm-run-all generate type-check lint",
    "build": "rimraf .cache && rimraf dist && parcel build src/index.html",
    "start": "parcel src/index.html",
    "dev:env": "export $(grep -v '^#' .env.development | xargs) && chmod +x env.sh && ./env.sh",
    "dev:server": "parcel --log-level verbose src/index.html",
    "dev": "rimraf .cache && rimraf dist && mkdir -p dist && npm-run-all generate type-check dev:env dev:server",
    "test": "jest --config=jest.config.js",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "api-spec-converter": "^2.12.0",
    "buffer": "^5.5.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "events": "^3.1.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-unit": "^0.0.2",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "parcel": "2.8.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "shx": "^0.3.4",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^5.1.0",
    "typescript": "^4.1.2",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.69",
    "@mui/material": "^5.13.4",
    "@mui/system": "^5.13.2",
    "@pagopa/mui-italia": "0.6.10",
    "@pagopa/openapi-codegen-ts": "12.1.2",
    "@types/jest": "^29.5.5",
    "card-validator": "^8.1.1",
    "dotenv": "^16.0.3",
    "formik": "^2.4.2",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.4",
    "italia-tslint-rules": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-router-dom": "^6.6.1",
    "rimraf": "^5.0.5",
    "ts-adt": "^2.1.2",
    "ts-node": "^10.9.1"
  }
}
